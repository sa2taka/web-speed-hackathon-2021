{
  "private": true,
  "name": "@web-speed-hackathon-2021/server",
  "version": "0.0.1",
  "license": "MPL-2.0",
  "main": "lib/index.js",
  "scripts": {
    "build": "esbuild --bundle --outdir=lib  --sourcemap --format=cjs --platform=node --external:mock-aws-s3 --external:aws-sdk --external:nock --external:pg-hstore src/index.ts ",
