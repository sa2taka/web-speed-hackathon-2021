    "start": "node lib/index.js",
    "prestart:clean": "rimraf ../upload",
    "prestart:init": "mkdirp ../upload/images ../upload/movies ../upload/sounds"
  },
  "dependencies": {
    "@ffmpeg/core": "0.10.0",
    "@ffmpeg/ffmpeg": "0.10.1",
    "bcrypt": "5.0.1",
    "body-parser": "1.19.0",
    "connect-history-api-fallback": "1.6.0",
    "express": "4.17.1",
    "express-promise-router": "4.1.0",
    "express-session": "1.17.2",
    "http-errors": "1.8.1",
    "iconv-lite": "0.6.3",
    "jschardet": "3.0.0",
    "mkdirp": "1.0.4",
    "music-metadata": "7.11.4",
    "npm-run-all": "4.1.5",
    "rimraf": "3.0.2",
    "sequelize": "6.11.0",
    "serve-static": "1.14.1",
    "sharp": "0.29.3",
    "sqlite3": "5.0.2",
    "ulid": "2.3.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/bcrypt": "10.0.28-alpha",
    "@types/body-parser": "1.19.2",
    "@types/connect-history-api-fallback": "1.3.5",
    "@types/express": "4.17.13",
    "@types/express-session": "1.17.4",
    "@types/http-errors": "1.8.1",
    "@types/node": "^16.11.12",
    "@types/serve-static": "1.13.10",
    "@types/sharp": "0.29.4",
    "@types/sqlite3": "3.1.7",
    "@types/uuid": "8.3.3",
    "@webpack-cli/generators": "^2.4.1",
    "esbuild": "^0.14.5",
    "node-gyp": "^8.4.1",
    "pg-hstore": "^2.3.4",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.4",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.6.0"
  },
  "engines": {
    "node": "16.x.x"
  },
  "typesync": {
    "ignoreDeps": [
      "dev"
    ],
    "ignorePackages": [
      "@babel/core",
      "@babel/node",
      "@babel/preset-env",
      "rimraf",
      "mkdirp",
      "npm-run-all"
    ]
